<%= form_for(@student) do |f| %>
<div class="col-sm-12 ">
  <div class="col-sm-8 center-block" style="margin-bottom: 40px;">
    <div class="form-group">
      <%= f.label :first_name, class: "col-sm-3"%>
      <%= content_tag :div, class: "col-sm-9" do %>
      <%= f.text_field :first_name, class: "form-control"%>
      <% end %>
    </div>
    <div class="form-group">
      <%= f.label :last_name, class: "col-sm-3"%>
      <%= content_tag :div, class: "col-sm-9" do %>
      <%= f.text_field :last_name, class: "form-control"%>
      <% end %>
    </div>
    <div class="form-group">
      <%= f.label :classroom, class: "col-sm-3"%>
      <%= content_tag :div, class: "col-sm-9" do %>
      <%= f.text_field :classroom, class: "form-control"%>
      <% end %>
    </div>
  </div>
  <%= f.submit class: "btn btn-primary center-block", style: "width: 200px"%>
</div>
<%end%>
